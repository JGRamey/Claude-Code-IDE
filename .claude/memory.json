      "sla": "2_hour_review",
      "dependencies": ["orchestrator"],
      "status": "active"
    },
    "database-specialist": {
      "role": "database_architect",
      "model": "sonnet",
      "responsibilities": ["neon_postgresql", "drizzle_orm", "supabase_migration", "schema_design"],
      "sla": "6_hour_database",
      "dependencies": ["backend-architect"],
      "status": "active"
    },
    "devops-specialist": {
      "role": "deployment_infrastructure",
      "model": "sonnet",
      "responsibilities": ["squarespace_deployment", "custom_code_integration", "performance_optimization", "site_infrastructure"],
      "sla": "4_hour_deployment",
      "dependencies": ["orchestrator"],
      "status": "active"
    },
    "ui-ux": {
      "role": "design_user_experience",
      "model": "opus",
      "responsibilities": ["ui_design", "ux_optimization", "design_systems", "accessibility", "visual_consistency"],
      "sla": "6_hour_design",
      "dependencies": ["frontend-architect"],
      "status": "active"
    }
  },
  
  "workflows": {
    "full_stack_feature": {
      "steps": [
        {"agent": "orchestrator", "action": "analyze_requirements"},
        {"agent": "backend-architect", "action": "design_api"},
        {"agent": "frontend-architect", "action": "design_ui"},
        {"parallel": [
          {"agent": "backend-architect", "action": "implement_api"},
          {"agent": "frontend-architect", "action": "implement_ui"}
        ]},
        {"agent": "cli-integration", "action": "connect_systems"},
        {"agent": "testing-specialist", "action": "create_tests"},
        {"agent": "performance-monitor", "action": "optimize"},
        {"agent": "documentor", "action": "document_feature"},
        {"agent": "evaluator", "action": "final_review"}
      ],
      "estimated_duration": "2_days"
    },
    "bug_fix": {
      "steps": [
        {"agent": "evaluator", "action": "analyze_bug"},
        {"agent": "orchestrator", "action": "assign_specialist"},
        {"agent": "dynamic", "action": "implement_fix"},
        {"agent": "testing-specialist", "action": "regression_test"},
        {"agent": "documentor", "action": "update_changelog"}
      ],
      "estimated_duration": "4_hours"
    },
    "performance_optimization": {
      "steps": [
        {"agent": "performance-monitor", "action": "profile_application"},
        {"agent": "evaluator", "action": "identify_bottlenecks"},
        {"parallel": [
          {"agent": "frontend-architect", "action": "optimize_client"},
          {"agent": "backend-architect", "action": "optimize_server"}
        ]},
        {"agent": "testing-specialist", "action": "performance_tests"},
        {"agent": "documentor", "action": "document_improvements"}
      ],
      "estimated_duration": "1_day"
    }
  },
  
  "patterns": {
    "context7": {
      "description": "Advanced coding patterns for Claude Code IDE",
      "rules": [
        "Always use TypeScript strict mode",
        "Implement error boundaries for all components",
        "Use virtual scrolling for lists > 100 items",
        "Lazy load components not in initial viewport",
        "Memoize expensive computations",
        "Implement proper cleanup in useEffect",
        "Use compound components for complex UI",
        "Apply repository pattern for data access",
        "Implement command queue for CLI operations",
        "Use WebSocket for real-time updates"
      ]
    }
  },
  
