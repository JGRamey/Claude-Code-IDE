{
  "project": {
    "name": "Claude Code IDE",
    "version": "1.0.0",
    "description": "A fully functional local interactive IDE specifically designed for Claude Code CLI integration with Context7 MCP server",
    "type": "fullstack",
    "status": "production_ready",
    "created": "2024-01-15",
    "lastModified": "2025-01-02",
    "repository": "https://github.com/your-org/claude-code-ide"
  },
  
  "architecture": {
    "frontend": {
      "framework": "React 18",
      "language": "TypeScript 5",
      "bundler": "Vite",
      "stateManagement": "Zustand",
      "styling": "TailwindCSS",
      "editor": "Monaco Editor",
      "testing": ["Jest", "React Testing Library", "Playwright"]
    },
    "backend": {
      "runtime": "Node.js 20",
      "framework": "Express",
      "websocket": "ws",
      "database": "PostgreSQL",
      "orm": "Prisma",
      "caching": "Redis"
    },
    "infrastructure": {
      "containerization": "Docker",
      "orchestration": "Docker Compose",
      "ci_cd": "GitHub Actions",
      "monitoring": "Prometheus + Grafana"
    }
  },
  
