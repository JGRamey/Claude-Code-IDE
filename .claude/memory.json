{
  "project": {
    "name": "Claude Code IDE",
    "version": "1.0.0",
    "description": "A fully functional local interactive IDE specifically designed for Claude Code CLI integration",
    "type": "fullstack",
    "status": "active_development",
    "created": "2024-01-15",
    "lastModified": "2024-01-20",
    "repository": "https://github.com/your-org/claude-code-ide"
  },
  
  "architecture": {
    "frontend": {
      "framework": "React 18",
      "language": "TypeScript 5",
      "bundler": "Vite",
      "stateManagement": "Zustand",
      "styling": "TailwindCSS",
      "editor": "Monaco Editor",
      "testing": ["Jest", "React Testing Library", "Playwright"]
    },
    "backend": {
      "runtime": "Node.js 20",
      "framework": "Express",
      "websocket": "ws",
      "database": "PostgreSQL",
      "orm": "Prisma",
      "caching": "Redis"
    },
    "infrastructure": {
      "containerization": "Docker",
      "orchestration": "Docker Compose",
      "ci_cd": "GitHub Actions",
      "monitoring": "Prometheus + Grafana"
    }
  },
  
  "agents": {
    "orchestrator": {
      "role": "master_coordinator",
      "model": "opus",
      "responsibilities": ["task_distribution", "quality_gates", "timeline_management"],
      "sla": "5_minute_response",
      "dependencies": [],
      "status": "active"
    },
    "frontend-architect": {
      "role": "ui_specialist",
      "model": "opus",
      "responsibilities": ["react_development", "monaco_integration", "state_management"],
      "sla": "4_hour_feature",
      "dependencies": ["orchestrator"],
      "status": "active"
    },
    "backend-architect": {
      "role": "api_specialist",
      "model": "opus",
      "responsibilities": ["api_development", "websocket_setup", "database_design"],
      "sla": "4_hour_feature",
      "dependencies": ["orchestrator"],
      "status": "active"
    },
    "cli-integration": {
      "role": "claude_cli_specialist",
      "model": "opus",
      "responsibilities": ["cli_process_management", "command_execution", "response_parsing"],
      "sla": "2_hour_integration",
      "dependencies": ["orchestrator", "backend-architect"],
      "status": "active"
    },
    "testing-specialist": {
      "role": "quality_assurance",
      "model": "sonnet",
      "responsibilities": ["test_creation", "coverage_optimization", "e2e_testing"],
      "sla": "8_hour_suite",
      "dependencies": ["frontend-architect", "backend-architect"],
      "status": "active"
    },
    "performance-monitor": {
      "role": "optimization_specialist",
      "model": "sonnet",
      "responsibilities": ["profiling", "bottleneck_identification", "optimization"],
      "sla": "6_hour_analysis",
      "dependencies": ["evaluator"],
      "status": "active"
    },
    "documentor": {
      "role": "knowledge_manager",
      "model": "sonnet",
      "responsibilities": ["documentation", "session_continuity", "knowledge_base"],
      "sla": "1_hour_documentation",
      "dependencies": ["all_agents"],
      "status": "active"
    },
    "evaluator": {
      "role": "quality_analyst",
      "model": "sonnet",
      "responsibilities": ["code_review", "metrics_tracking", "improvement_recommendations"],
      "sla": "2_hour_review",
      "dependencies": ["orchestrator"],
      "status": "active"
    }
  },
  
  "workflows": {
    "full_stack_feature": {
      "steps": [
        {"agent": "orchestrator", "action": "analyze_requirements"},
        {"agent": "backend-architect", "action": "design_api"},
        {"agent": "frontend-architect", "action": "design_ui"},
        {"parallel": [
          {"agent": "backend-architect", "action": "implement_api"},
          {"agent": "frontend-architect", "action": "implement_ui"}
        ]},
        {"agent": "cli-integration", "action": "connect_systems"},
        {"agent": "testing-specialist", "action": "create_tests"},
        {"agent": "performance-monitor", "action": "optimize"},
        {"agent": "documentor", "action": "document_feature"},
        {"agent": "evaluator", "action": "final_review"}
      ],
      "estimated_duration": "2_days"
    },
    "bug_fix": {
      "steps": [
        {"agent": "evaluator", "action": "analyze_bug"},
        {"agent": "orchestrator", "action": "assign_specialist"},
        {"agent": "dynamic", "action": "implement_fix"},
        {"agent": "testing-specialist", "action": "regression_test"},
        {"agent": "documentor", "action": "update_changelog"}
      ],
      "estimated_duration": "4_hours"
    },
    "performance_optimization": {
      "steps": [
        {"agent": "performance-monitor", "action": "profile_application"},
        {"agent": "evaluator", "action": "identify_bottlenecks"},
        {"parallel": [
          {"agent": "frontend-architect", "action": "optimize_client"},
          {"agent": "backend-architect", "action": "optimize_server"}
        ]},
        {"agent": "testing-specialist", "action": "performance_tests"},
        {"agent": "documentor", "action": "document_improvements"}
      ],
      "estimated_duration": "1_day"
    }
  },
  
  "patterns": {
    "context7": {
      "description": "Advanced coding patterns for Claude Code IDE",
      "rules": [
        "Always use TypeScript strict mode",
        "Implement error boundaries for all components",
        "Use virtual scrolling for lists > 100 items",
        "Lazy load components not in initial viewport",
        "Memoize expensive computations",
        "Implement proper cleanup in useEffect",
        "Use compound components for complex UI",
        "Apply repository pattern for data access",
        "Implement command queue for CLI operations",
        "Use WebSocket for real-time updates"
      ]
    }
  },
  
  "quality_gates": {
    "code_review": {
      "required_approvals": 2,
      "checks": ["linting", "type_checking", "test_coverage", "security_scan"]
    },
    "test_coverage": {
