      "sla": "4_hour_feature",
      "dependencies": ["orchestrator"],
      "status": "active"
    },
    "backend-architect": {
      "role": "api_specialist",
      "model": "opus",
      "responsibilities": ["api_development", "websocket_setup", "express_server", "context7_mcp_integration"],
      "sla": "4_hour_feature",
      "dependencies": ["orchestrator"],
      "status": "active"
    },
    "cli-integrator": {
      "role": "claude_cli_specialist",
